root = true

# noinspection EditorConfigKeyCorrectness
[*.cs]
dotnet_diagnostic.CS0219.severity = silent
dotnet_diagnostic.CS0168.severity = silent
dotnet_diagnostic.CS0162.severity = silent
dotnet_diagnostic.CS0183.severity = silent
dotnet_diagnostic.CS0184.severity = silent
dotnet_diagnostic.CS1981.severity = silent
dotnet_diagnostic.CS1718.severity = silent

csharp_default_private_modifier = implicit

resharper_redundant_jump_statement_highlighting = none
resharper_empty_constructor_highlighting = none
resharper_redundant_logical_conditional_expression_operand_highlighting = none
resharper_unused_parameter_local_highlighting = none
resharper_constant_conditional_access_qualifier_highlighting = none

resharper_heuristic_unreachable_code_highlighting = hint
resharper_unused_variable_highlighting = hint
resharper_unused_member_local_highlighting = hint
resharper_object_creation_as_statement_highlighting = hint
resharper_not_accessed_variable_highlighting = hint
resharper_redundant_assignment_highlighting = hint
resharper_inconsistent_naming_highlighting = hint
resharper_condition_is_always_true_or_false_highlighting = hint
resharper_double_negation_operator_highlighting = hint
resharper_class_never_instantiated_global_highlighting = hint
resharper_replace_with_simple_assignment_false_highlighting = hint
resharper_replace_with_simple_assignment_true_highlighting = hint